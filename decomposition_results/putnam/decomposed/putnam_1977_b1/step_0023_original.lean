theorem h₅ (N✝ : ℤ) (hN✝ : N✝ ≥ 2) (h₁ : ∀ n ≥ 2, (↑n ^ 3 - 1) / (↑n ^ 3 + 1) = (↑n - 1) / (↑n + 1) * ((↑n + 1) ^ 2 - (↑n + 1) + 1) / (↑n ^ 2 - ↑n + 1)) (N : ℤ) (hN : N ≥ 2) (h₄ : ∀ (K : ℤ), 2 ≤ K → ∏ n ∈ Finset.Icc 2 K, (↑n ^ 3 - 1) / (↑n ^ 3 + 1) = 2 * (↑K ^ 2 + ↑K + 1) / (3 * ↑K * (↑K + 1))) : ∏ n ∈ Finset.Icc 2 N, (↑n ^ 3 - 1) / (↑n ^ 3 + 1) = 2 * (↑N ^ 2 + ↑N + 1) / (3 * ↑N * (↑N + 1)) :=
  by
  have h₆ := h₄ N hN
  exact h₆