theorem h₆₆ (N : ℕ) (hN : N = ∑ i ∈ Finset.range 1998, 10 ^ i) (h₁ : N = (10 ^ 1998 - 1) / 9) (h₂ : 10 ^ 1998 > 1) (h₃ : 10 ^ 1999 > 7) (h₄ : 10 ^ 1998 - 1 > 0) (h₅ : ↑N = (10 ^ 1998 - 1) / 9) (h₆₁ : 0 < 10 ^ 1998 - 1) (h₆₂ : 0 < 10 ^ 1000) (h₆₃ : 0 < 10 ^ 1998) (h₆₄ : 0 < 10 ^ 1999) (h₆₅ : 0 < √↑N) : √↑N < 10 ^ 999 :=
  by
  have h₆₆₁ : Real.sqrt N < ((10 : ℝ) ^ 999) := by sorry
  exact h₆₆₁