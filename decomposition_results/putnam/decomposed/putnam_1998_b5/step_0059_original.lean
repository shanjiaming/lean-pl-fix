theorem h₈₁ (N : ℕ) (hN : N = ∑ i ∈ Finset.range 1998, 10 ^ i) (h₁ : N = (10 ^ 1998 - 1) / 9) (h₂ : 10 ^ 1998 > 1) (h₃ : 10 ^ 1999 > 7) (h₄ : 10 ^ 1998 - 1 > 0) (h₅ : ↑N = (10 ^ 1998 - 1) / 9) (h₆ : 10 ^ 1000 * √↑N < (10 ^ 1999 - 4) / 3) (h₇ : (10 ^ 1999 - 7) / 3 < 10 ^ 1000 * √↑N) : ↑⌊10 ^ 1000 * √↑N⌋₊ ≤ 10 ^ 1000 * √↑N := by
  exact Nat.floor_le (by positivity)