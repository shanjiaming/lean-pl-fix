theorem h₈ (h_subset₁ :  {f | Differentiable ℝ f ∧ ∀ (x : ℝ), ∀ n > 0, deriv f x = (f (x + ↑n) - f x) / ↑n} ⊆    {f | ∃ c d, ∀ (x : ℝ), f x = c * x + d}) (f : ℝ → ℝ) (hf : f ∈ {f | ∃ c d, ∀ (x : ℝ), f x = c * x + d}) (c d : ℝ) (h₂ : ∀ (x : ℝ), f x = c * x + d) (h₃ : Differentiable ℝ f) (x : ℝ) (n : ℤ) (hn : n > 0) (h₅ : deriv f x = c) (h₇ : f (x + ↑n) = c * (x + ↑n) + d) : f x = c * x + d := by rw [h₂ x] <;> ring