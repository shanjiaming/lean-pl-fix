theorem h₆₀ (n k : ℕ) (hnk : n > 0 ∧ k > 0) (h₁ : n > 0) (h₂ : k > 0) (h₃ : 2 * k + 1 > 0) (h : k + 1 < n) (h₄ h₅₁ : n > k + 1) (h₅₂ : ∀ (s : Equiv.Perm (Fin n)), klimited k n s → s = 1) (h₅₈ : {s | klimited k n s} = {1}) (h₅₉ : {s | klimited k n s}.ncard = 1) : Odd {s | klimited k n s}.ncard := by
  rw [h₅₉] <;> simp [Nat.odd_iff_not_even, parity_simps]