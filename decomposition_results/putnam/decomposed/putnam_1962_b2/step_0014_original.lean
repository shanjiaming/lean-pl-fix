theorem h₅₃ (q : ℕ+ → ℚ) (hq : Function.Bijective q) (hq' : Function.Surjective q) (a b : ℝ) (hab : a < b) (h₁ : {n | (↑(q n) : ℝ) < a} ⊆ {n | (↑(q n) : ℝ) < b}) (r : ℚ) (hr₁ : a < (↑r : ℝ)) (hr₂ : (↑r : ℝ) < b) (m : ℕ+) (hm : q m = r) (h₅₁ : (↑(q m) : ℝ) = (↑r : ℝ)) (h₅₂ : (↑r : ℝ) < b) : (↑(q m) : ℝ) < b := by simpa [h₅₁] using h₅₂