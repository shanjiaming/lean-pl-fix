theorem h₁₀ (P : Polynomial ℤ) (h_coeff : ∀ (n : ℕ), P.coeff n = 0 ∨ P.coeff n = 1) (h_factor : ∃ Q R, Q.degree > 0 ∧ R.degree > 0 ∧ P = Q * R) (h₁ : Polynomial.eval 2 P ≠ 0) (h h₃ : Polynomial.eval 2 P = 1) (Q R : Polynomial ℤ) (hQ : Q.degree > 0) (hR : R.degree > 0) (hP : P = Q * R) (h₅ : 1 = Polynomial.eval 2 Q * Polynomial.eval 2 R) (h₇ : (Polynomial.eval 2 Q).natAbs ∣ Int.natAbs 1) (h₉ : (Polynomial.eval 2 Q).natAbs ∣ 1) : (Polynomial.eval 2 Q).natAbs = 1 := by exact Nat.dvd_one.mp h₉