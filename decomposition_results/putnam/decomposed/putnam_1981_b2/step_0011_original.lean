theorem h₅ (P : ℝ × ℝ × ℝ → Prop) (hP :  P = fun x =>    match x with    | (r, s, t) => 1 ≤ r ∧ r ≤ s ∧ s ≤ t ∧ t ≤ 4) (f : ℝ × ℝ × ℝ → ℝ) (hf :  f = fun x =>    match x with    | (r, s, t) => (r - 1) ^ 2 + (s / r - 1) ^ 2 + (t / s - 1) ^ 2 + (4 / t - 1) ^ 2) (h₁ : (√2 - 1) ^ 2 = 3 - 2 * √2) (h₂ : (2 / √2 - 1) ^ 2 = 3 - 2 * √2) (h₃ : (2 * √2 / 2 - 1) ^ 2 = 3 - 2 * √2) (h₄ : (4 / (2 * √2) - 1) ^ 2 = 3 - 2 * √2) : (√2 - 1) ^ 2 + (2 / √2 - 1) ^ 2 + (2 * √2 / 2 - 1) ^ 2 + (4 / (2 * √2) - 1) ^ 2 = 12 - 8 * √2 :=
  by nlinarith [Real.sqrt_nonneg 2, Real.sq_sqrt (show 0 ≤ 2 by norm_num)]