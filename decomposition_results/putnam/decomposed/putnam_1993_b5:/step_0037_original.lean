theorem h₂₄₁ (p : Fin 4 → EuclideanSpace ℝ (Fin 2)) (h : ∀ (i j : Fin 4), i ≠ j → ∃ n, dist (p i) (p j) = ↑n ∧ Odd n) (n₁ : ℤ) (hn₁ : dist (p ⟨0, ⋯⟩) (p ⟨1, ⋯⟩) = ↑n₁) (hn₁_odd : Odd n₁) (n₂ : ℤ) (hn₂ : dist (p ⟨0, ⋯⟩) (p ⟨2, ⋯⟩) = ↑n₂) (hn₂_odd : Odd n₂) (n₃ : ℤ) (hn₃ : dist (p ⟨0, ⋯⟩) (p ⟨3, ⋯⟩) = ↑n₃) (hn₃_odd : Odd n₃) (n₄ : ℤ) (hn₄ : dist (p ⟨1, ⋯⟩) (p ⟨2, ⋯⟩) = ↑n₄) (hn₄_odd : Odd n₄) (n₅ : ℤ) (hn₅ : dist (p ⟨1, ⋯⟩) (p ⟨3, ⋯⟩) = ↑n₅) (hn₅_odd : Odd n₅) (n₆ : ℤ) (hn₆ : dist (p ⟨2, ⋯⟩) (p ⟨3, ⋯⟩) = ↑n₆) (hn₆_odd : Odd n₆) (h₇ : dist (p ⟨0, ⋯⟩) (p ⟨1, ⋯⟩) = ↑n₁) (h₈ : dist (p ⟨0, ⋯⟩) (p ⟨2, ⋯⟩) = ↑n₂) (h₉ : dist (p ⟨0, ⋯⟩) (p ⟨3, ⋯⟩) = ↑n₃) (h₁₀ : dist (p ⟨1, ⋯⟩) (p ⟨2, ⋯⟩) = ↑n₄) (h₁₁ : dist (p ⟨1, ⋯⟩) (p ⟨3, ⋯⟩) = ↑n₅) (h₁₂ : dist (p ⟨2, ⋯⟩) (p ⟨3, ⋯⟩) = ↑n₆) (h₁₃ : n₁ % 2 = 1) (h₁₄ : n₂ % 2 = 1) (h₁₅ : n₃ % 2 = 1) (h₁₆ : n₄ % 2 = 1) (h₁₇ : n₅ % 2 = 1) (h₁₈ : n₆ % 2 = 1) (h₁₉ : n₁ ^ 2 % 4 = 1) (h₂₀ : n₂ ^ 2 % 4 = 1) (h₂₁ : n₃ ^ 2 % 4 = 1) (h₂₂ : n₄ ^ 2 % 4 = 1) (h₂₃ : n₅ ^ 2 % 4 = 1) : n₆ % 4 = 1 ∨ n₆ % 4 = 3 :=
  by
  have h₂₄₂ : (n₆ : ℤ) % 2 = 1 := h₁₈
  have h₂₄₃ : (n₆ : ℤ) % 4 = 1 ∨ (n₆ : ℤ) % 4 = 3 := by sorry
  exact h₂₄₃