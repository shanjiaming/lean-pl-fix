theorem h₁₆ (h₀₅ : (fun x => (Real.cos (2 * x - 6) - 1) / (x - 3) ^ 2) = fun x => -2 * (Real.sin (x - 3) / (x - 3)) ^ 2) (h₀₇ h₁₀ h₁₃ : Tendsto (fun x => Real.sin (x - 3) / (x - 3)) (𝓝[≠] 3) (𝓝 1)) (h₁₅ : Continuous fun y => y ^ 2) : Tendsto (fun x => (Real.sin (x - 3) / (x - 3)) ^ 2) (𝓝[≠] 3) (𝓝 (1 ^ 2)) := by
  apply h₁₅.continuousAt.tendsto.comp h₁₃